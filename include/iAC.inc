hook OnGameModeInit()
{
	print("\n\nSerwer jest chroniony przez system iAC.\nWersja: "IAC_VERSION"\nAutor: Inferno\n\n");

	SetTimer("IAC_UpdateAntyCheat", IAC_UPDATE_INTERVAL, true);
	SetTimer("IAC_MinTimer", 60*1000, true);
	return 1;
}

hook OnGameModeExit()
{

	return 1;
}

hook OnPlayerConnect(playerid)
{
	SetPlayerImmunity(playerid, false);

	iAC_PlayerInfo[playerid][getPos] = false;
	iAC_PlayerInfo[playerid][pFreeze] = false;
	iAC_PlayerInfo[playerid][pSpectate] = false;
	iAC_PlayerInfo[playerid][CountDeaths] = 0;
	iAC_PlayerInfo[playerid][StartDeath] = 0;
	iAC_PlayerInfo[playerid][pRconFails] = 0;

	for(new i; i < IAC_MAX_CHEATS; i++)
	{
		if(iAC_PlayerInfo[playerid][warning][i] != 0) iAC_PlayerInfo[playerid][warning][CHEAT_FLY] = 0;
	}
	return 1;
}

hook OnPlayerDisconnect(playerid, reason)
{
	if(!reason) Kick(playerid);
	new ip[16];
	GetPlayerIp(playerid, ip, 16);
	BlockIpAddress(ip, (10 * 1000) - (reason ? 0 : GetConsoleVarAsInt("playertimeout")));
	return 1;
}

hook OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
	iAC_PlayerInfo[playerid][pCarAC] = vehicleid;
}

hook OnPlayerExitVehicle(playerid, vehicleid)
{
	iAC_PlayerInfo[playerid][pCarAC] = INVALID_VEHICLE_ID;
}

hook OnPlayerDeath(playerid, killerid, reason)
{
	if(!IsPlayerNPC(playerid))
    {
        if(gettime() - iAC_PlayerInfo[playerid][StartDeath] < 5)
        {
            iAC_PlayerInfo[playerid][CountDeaths]++;
            if(iAC_PlayerInfo[playerid][CountDeaths] == 10) 
            {
                CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_FAKEKILL, "null");
                iAC_PlayerInfo[playerid][CountDeaths] = 0;
                return 1;  
            }
        }
        else iAC_PlayerInfo[playerid][CountDeaths] = 1;
        iAC_PlayerInfo[playerid][StartDeath] = gettime();
    }
	return 1;
}

hook OnRconLoginAttempt(ip[], password[], success)
{
	new playerid = GetPlayerByIP(ip);
	if(playerid != INVALID_PLAYER_ID)
	{
		if(!success)
		{
			iAC_PlayerInfo[playerid][warning][CHEAT_RCON]++;
			if(iAC_PlayerInfo[playerid][warning][CHEAT_RCON] == IAC_WARNING_LIMIT)
			{
				CallLocalFunction("OnCheatWarning", "dddds", playerid, CHEAT_RCON, iAC_PlayerInfo[playerid][warning][CHEAT_RCON], IAC_WARNING_LIMIT, "null");
			}
			else
			{
				CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_RCON, "null");
				iAC_PlayerInfo[playerid][warning][CHEAT_RCON] = 0;
			}
		}
		else
		{
			iAC_PlayerInfo[playerid][warning][CHEAT_RCON] = 0;
		}
	}
}

hook OnVehicleMod(playerid, vehicleid, componentid)
{
	if(!iAC_IsComponentidCompatible(GetVehicleModel(vehicleid), componentid))
	{
		CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_VEHMOD, "null");
	}
}

hook OnPlayerStateChange(playerid, newstate, oldstate)
{
	new iAC_vehicleid = GetPlayerVehicleID(playerid);
	if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER) iAC_PlayerInfo[playerid][pCarAC] = iAC_vehicleid;

	if(newstate == PLAYER_STATE_DRIVER)
	{
		new iAC_seatid = GetPlayerVehicleSeat(playerid);
		if(iAC_seatid != 128)
		{
			new iAC_model = GetVehicleModel(iAC_vehicleid) - 400;
			if(iAC_seatid || iAC_model < 0 || iAC_MaxPassengers[iAC_model >>> 3] >>> ((iAC_model & 7) << 2) & 0xF == 15) 
			{
				CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_SEATCRASH, "null");
			}
		}
	}
	if(newstate == PLAYER_STATE_PASSENGER)
	{
		new iAC_seatid = GetPlayerVehicleSeat(playerid);
		new iAC_model = GetVehicleModel(GetPlayerVehicleID(playerid)) - 400;
		if(iAC_model < 0)
		{
			CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_SEATCRASH, "null");
		}
		else
		{
			iAC_model = (iAC_MaxPassengers[iAC_model >>> 3] >>> ((iAC_model & 7) << 2)) & 0xF;
			if(iAC_seatid < 1 || (iAC_model == 15 || iAC_seatid > iAC_model)) 
			{
				CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_SEATCRASH, "null");
			}
		}
	}
}

stock iAC_IsComponentidCompatible(modelid, componentid)
{
	if(modelid)
	{
		switch(componentid)
		{
			case 1000..1191:
			{
				componentid -= 1000;
				if(iAC_VehicleMods[modelid - 400][componentid >>> 5]
				& (1 << (componentid & 0b00011111))) return true;
			}
			case 1192, 1193:
			{
				if(modelid == 576) return true;
			}
		}
	}
	return false;
}

stock GetPlayerByIP(ip[])
{
	foreach(new playerid : Player)
	{
		new iAC_PlayerIP[16];
		GetPlayerIp(playerid, iAC_PlayerIP, 16);
		if(!strcmp(iAC_PlayerIP, ip))
		{
			return playerid;
		}
	}
	return INVALID_PLAYER_ID;
}

function IAC_UpdateAntyCheat()
{
	foreach(new playerid : Player)
	{
		if(!GetPlayerImmunity(playerid))
		{
			new Float: iAC_Pos[3], 
				iAC_anim = GetPlayerAnimationIndex(playerid),
				iAC_speed = GetPlayerSpeed(playerid, true);
			GetPlayerPos(playerid, iAC_Pos[0], iAC_Pos[1], iAC_Pos[2]);

			if(iAC_Pos[2] > 120.0 && (iAC_anim == 1543 || iAC_anim == 1538 || iAC_anim == 1539))
			{
				iAC_PlayerInfo[playerid][warning][CHEAT_FLY]++;
				if(iAC_PlayerInfo[playerid][warning][CHEAT_FLY] == IAC_WARNING_LIMIT)
				{
					CallLocalFunction("OnCheatWarning", "dddds", playerid, CHEAT_FLY, iAC_PlayerInfo[playerid][warning][CHEAT_FLY], IAC_WARNING_LIMIT, "null");
				}
				else
				{
					CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_FLY, "null");
					iAC_PlayerInfo[playerid][warning][CHEAT_FLY] = 0;
				}
				continue;
			}

			if(iAC_PlayerInfo[playerid][getPos])
			{
				if(GetPlayerSurfingVehicleID(playerid) == INVALID_VEHICLE_ID)
				{
					new iAC_distance = floatround(GetPlayerDistanceFromPoint(playerid, iAC_PlayerInfo[playerid][lastPos][0], iAC_PlayerInfo[playerid][lastPos][1], iAC_PlayerInfo[playerid][lastPos][2]));
					if(iAC_distance >= 35 && iAC_distance < 250 && iAC_speed < 30)
					{
						iAC_PlayerInfo[playerid][warning][CHEAT_AIRBREAK]++;
						new iAC_params[64];
						format(iAC_params, 64, "%.2f|%d", iAC_Pos[2], iAC_speed);
						if(iAC_PlayerInfo[playerid][warning][CHEAT_AIRBREAK] == IAC_WARNING_LIMIT)
						{
							CallLocalFunction("OnCheatWarning", "dddds", playerid, CHEAT_AIRBREAK, iAC_PlayerInfo[playerid][warning][CHEAT_AIRBREAK], IAC_WARNING_LIMIT, iAC_params);
						}
						else
						{
							CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_AIRBREAK, iAC_params);
							iAC_PlayerInfo[playerid][warning][CHEAT_AIRBREAK] = 0;
						}
						continue;
					}
				}
			}

			if(GetTogglePlayerControllableEx(playerid))
			{
				if(GetPlayerState(playerid) != PLAYER_STATE_PASSENGER)
				{
					if(!IsPlayerInRangeOfPoint(playerid, 3.0, iAC_PlayerInfo[playerid][pFreezePos][0], iAC_PlayerInfo[playerid][pFreezePos][1], iAC_PlayerInfo[playerid][pFreezePos][2]))
					{
						if(iAC_speed >= 5)
						{
							iAC_PlayerInfo[playerid][warning][CHEAT_FREEZE]++;
							new iAC_params[64];
							format(iAC_params, 64, "%f|%f|%f", iAC_PlayerInfo[playerid][pFreezePos][0], iAC_PlayerInfo[playerid][pFreezePos][1], iAC_PlayerInfo[playerid][pFreezePos][2]);
							if(iAC_PlayerInfo[playerid][warning][CHEAT_FREEZE] == IAC_WARNING_LIMIT)
							{
								CallLocalFunction("OnCheatWarning", "dddds", playerid, CHEAT_FREEZE, iAC_PlayerInfo[playerid][warning][CHEAT_FREEZE], IAC_WARNING_LIMIT, iAC_params);
							}
							else
							{
								CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_FREEZE, iAC_params);
								iAC_PlayerInfo[playerid][warning][CHEAT_FREEZE] = 0;
							}
						}
						else
						{
							GetPlayerPos(playerid, iAC_PlayerInfo[playerid][pFreezePos][0], iAC_PlayerInfo[playerid][pFreezePos][1], iAC_PlayerInfo[playerid][pFreezePos][2]);
						}
					}
				}
			}

			if(GetPlayerPing(playerid) > IAC_MAX_PING)
			{
				iAC_PlayerInfo[playerid][warning][CHEAT_PING]++;
				new iAC_params[64];
				format(iAC_params, 64, "%d", GetPlayerPing(playerid));
				if(iAC_PlayerInfo[playerid][warning][CHEAT_PING] == IAC_WARNING_LIMIT)
				{
					CallLocalFunction("OnCheatWarning", "dddds", playerid, CHEAT_PING, iAC_PlayerInfo[playerid][warning][CHEAT_PING], IAC_WARNING_LIMIT, iAC_params);
				}
				else
				{
					CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_PING, iAC_params);
					iAC_PlayerInfo[playerid][warning][CHEAT_PING] = 0;
				}
				continue;
			}

			if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT && iAC_PlayerInfo[playerid][pCarAC] != INVALID_VEHICLE_ID)
			{
				if(iAC_PlayerInfo[playerid][pCarAC] != GetPlayerVehicleID(playerid))
				{
					CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_CAR, "null");
					continue;
				}
			}

			if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING && !iAC_PlayerInfo[playerid][pSpectate])
			{
				CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_SPECTATE, "null");
				continue;
			}

			if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_USEJETPACK)
			{
				CallLocalFunction("OnCheatDetect", "dds", playerid, CHEAT_JETPACK, "null");
				continue;
			}

			iAC_PlayerInfo[playerid][lastPos][0] = iAC_Pos[0];
			iAC_PlayerInfo[playerid][lastPos][1] = iAC_Pos[1];
			iAC_PlayerInfo[playerid][lastPos][2] = iAC_Pos[2];
		}
	}
	return 1;
}

function IAC_MinTimer()
{
	foreach(new playerid : Player)
	{
		for(new i; i < IAC_MAX_CHEATS; i++)
		{
			if(iAC_PlayerInfo[playerid][warning][i] > 0) iAC_PlayerInfo[playerid][warning][CHEAT_FLY]--;
		}
	}
}

#define Speed(%0,%1,%2,%3,%4) \
		floatround(floatsqroot((%4)?(%0*%0+%1*%1+%2*%2):(%0*%0+%1*%1))*%3*1.6)

stock GetPlayerSpeed(playerid, get3d)
{
	new Float: x, Float: y, Float: z;
	if(IsPlayerInAnyVehicle(playerid)) 
	{
		GetVehicleVelocity(GetPlayerVehicleID(playerid), x, y, z);
	}
	else
	{
		GetPlayerVelocity(playerid, x, y, z);
	}
	return Speed(x, y, z, 100.0, get3d);
}

stock TogglePlayerControllableEx(playerid, bool: toggle)
{
	iAC_PlayerInfo[playerid][pFreeze] = toggle;
	GetPlayerPos(playerid, iAC_PlayerInfo[playerid][pFreezePos][0], iAC_PlayerInfo[playerid][pFreezePos][1], iAC_PlayerInfo[playerid][pFreezePos][2]);
	TogglePlayerControllable(playerid, toggle);
}

stock TogglePlayerSpectatingEx(playerid, bool: toggle)
{
	iAC_PlayerInfo[playerid][pSpectate] = toggle;
	TogglePlayerControllable(playerid, toggle);
}

stock GetTogglePlayerControllableEx(playerid) return iAC_PlayerInfo[playerid][pFreeze];

stock GetPlayerImmunity(playerid) return iAC_PlayerInfo[playerid][immu];
stock SetPlayerImmunity(playerid, bool: status) iAC_PlayerInfo[playerid][immu] = status;